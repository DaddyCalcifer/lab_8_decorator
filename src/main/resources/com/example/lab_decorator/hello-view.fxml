<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" prefHeight="238.0" prefWidth="358.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab_decorator.HelloController">
    
    <children>
        <VBox prefHeight="238.0" prefWidth="326.0">
            <children>
                <Pane fx:id="panel" prefHeight="213.0" prefWidth="272.0" />
                <HBox prefHeight="19.0" prefWidth="490.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#AddAll" text="Добавить все" />
                        <Button mnemonicParsing="false" onAction="#ClearAll" text="Удалить все" />
                    </children>
                </HBox>
            </children>
        </VBox>
        <VBox prefHeight="238.0" prefWidth="164.0">
            <children>
                <Button fx:id="girland" mnemonicParsing="false" onAction="#addLights" prefHeight="26.0" prefWidth="95.0" text="Гирлянда" />
                <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#addStar" prefHeight="26.0" prefWidth="95.0" text="Звезда" />
                <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#addPresents" prefHeight="26.0" prefWidth="95.0" text="Подарки" />
            </children>
        </VBox>
    </children>
</HBox>
